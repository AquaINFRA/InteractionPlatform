{
    "name": "starter",
    "private": true,
    "type": "module",
    "license": "Apache-2.0",
    "scripts": {
        "clean": "rimraf dist",
        "dev": "vite",
        "check-types": "tsc --noEmit",
        "watch-types": "pnpm check-types -w",
        "build": "tsc && vite build",
        "build-docs": "typedoc",
        "build-license-report": "ts-node --esm ./support/create-license-report.ts",
        "preview": "vite preview",
        "lint": "eslint ./src ./support --cache --ext .js,.ts,.jsx,.tsx,.mjs,.mts,.cjs,.cts",
        "prettier-check": "pnpm prettier:common -c",
        "prettier": "pnpm prettier:common -w",
        "prettier:common": "prettier ./src ./docs ./support --ignore-path .eslintignore --cache",
        "test": "vitest",
        "lint-shared-versions": "syncpack list-mismatches",
        "update-shared-versions": "syncpack fix-mismatches",
        "prepare": "husky install"
    },
    "pnpm": {
        "//1": "semver: https://github.com/advisories/GHSA-c2qf-rxjj-qqgw",
        "//2": "",
        "overrides": {
            "ol-mapbox-style": "workspace:disabled-package@*",
            "semver@<7.5.2": ">=7.5.2"
        }
    },
    "dependencies": {
        "@chakra-ui/icons": "^2.0.19",
        "@chakra-ui/react": "^2.6.1",
        "@chakra-ui/system": "^2.5.7",
        "@emotion/cache": "^11.11.0",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@formatjs/intl": "^2.7.2",
        "@open-pioneer/chakra-integration": "^1.0.0",
        "@open-pioneer/core": "^1.0.0",
        "@open-pioneer/integration": "^1.0.0",
        "@open-pioneer/runtime": "^1.0.0",
        "@open-pioneer/test-utils": "^1.0.0",
        "chakra-ui-markdown-renderer": "^4.1.0",
        "framer-motion": "^10.12.16",
        "ol": "^7.3.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.14.1",
        "react-use": "^17.4.0",
        "remark-gfm": "^4.0.0",
        "rehype": "13.0.1",
        "rehype-citation": "^2.0.0",
        "react-remark": "2.1.0",
        "html-react-parser": "5.0.6",
        "js-yaml": "^4.1.0"
    },
    "devDependencies": {
        "@open-pioneer/build-package-cli": "^1.0.1",
        "@open-pioneer/build-support": "^1.0.0",
        "@open-pioneer/vite-plugin-pioneer": "^1.0.2",
        "@testing-library/dom": "^9.3.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/js-yaml": "^4.0.5",
        "@types/node": "^16.18.34",
        "@types/react": "^18.2.7",
        "@types/react-dom": "^18.2.4",
        "@typescript-eslint/eslint-plugin": "^5.59.9",
        "@typescript-eslint/parser": "^5.59.9",
        "@vitejs/plugin-react-swc": "^3.3.2",
        "convert-csv-to-json": "^2.0.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-header": "^3.1.1",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "fast-glob": "^3.2.12",
        "handlebars": "^4.7.7",
        "husky": "^8.0.3",
        "js-yaml": "^4.1.0",
        "jsdom": "^22.1.0",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.1",
        "rollup-plugin-visualizer": "^5.9.0",
        "sass": "^1.63.2",
        "syncpack": "^10.5.1",
        "ts-node": "^10.9.1",
        "typedoc": "^0.24.8",
        "typescript": "~5.0.4",
        "vite": "^4.3.9",
        "vite-plugin-eslint": "^1.8.1",
        "vitest": "^0.31.4"
    },
    "engines": {
        "node": ">= 16",
        "pnpm": "^7 || ^8 || ^9"
    }
}
